{
  "name": "filebrowser-electron",
  "version": "0.9.0",
  "description": "A file browser",
  "author": "Brett Zamir",
  "contributors": [],
  "license": "AGPL-3.0-only",
  "main": "src/main.js",
  "type": "module",
  "browserslist": [
    "last 2 chrome versions"
  ],
  "scripts": {
    "tsc": "tsc",
    "lint": "npm run eslint --",
    "eslint": "eslint --ext=js,cjs,html,md .",
    "report": "c8 report",
    "build": "rollup -c && cp node_modules/miller-columns/miller-columns.css .",
    "pretest": "npm run build && rm -Rf coverage && rm -Rf .nyc_output",
    "test": "playwright test --config test/playwright.config.js && npm run report",
    "postinstall": "echo 'no longer needed to call ./rebuild.sh'",
    "rebuild": "./rebuild.sh",
    "start": "npm run build && electron-forge start --",
    "package": "electron-forge package",
    "make": "electron-forge make"
  },
  "repository": "https://github.com/brettz9/filebrowser-electron",
  "homepage": "https://github.com/brettz9/filebrowser-electron",
  "bugs": "https://github.com/brettz9/filebrowser-electron/issues",
  "keywords": [
    "Electron",
    "file",
    "browser",
    "filebrowser"
  ],
  "engines": {
    "node": ">=22.16.0"
  },
  "dependencies": {
    "@parcel/watcher": "^2.5.1",
    "base64-js": "^1.5.1",
    "electron-squirrel-startup": "^1.0.1",
    "filesize": "^11.0.13",
    "jamilih": "^0.63.1",
    "jquery": "^3.7.1",
    "jsdom": "^27.4.0",
    "load-stylesheets": "^0.13.0",
    "mdls-ts": "^2.3.1",
    "miller-columns": "0.17.0",
    "open-with-me": "^0.6.0",
    "plist": "^3.1.0",
    "stickynote": "^0.7.0",
    "system-icon2": "^0.5.1"
  },
  "devDependencies": {
    "@electron-forge/cli": "^7.10.2",
    "@electron-forge/maker-deb": "^7.10.2",
    "@electron-forge/maker-rpm": "^7.10.2",
    "@electron-forge/maker-squirrel": "^7.10.2",
    "@electron-forge/maker-zip": "^7.10.2",
    "@electron-forge/plugin-auto-unpack-natives": "^7.10.2",
    "@electron-forge/plugin-fuses": "^7.10.2",
    "@electron/fuses": "^1.8.0",
    "@electron/rebuild": "^4.0.2",
    "@playwright/test": "^1.57.0",
    "@rollup/plugin-commonjs": "^29.0.0",
    "@rollup/plugin-node-resolve": "^16.0.3",
    "@types/jquery": "^3.5.33",
    "@types/jsdom": "^27.0.0",
    "c8": "^10.1.3",
    "electron": "^39.2.7",
    "eslint": "^9.39.2",
    "eslint-config-ash-nazg": "^39.10.0",
    "playwright": "^1.57.0",
    "rollup": "^4.55.1",
    "typescript": "^5.9.3"
  }
}
